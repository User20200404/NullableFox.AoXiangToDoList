<?xml version="1.0" encoding="utf-8" ?>
<winex:WindowEx
    x:Class="NullableFox.AoXiangToDoList.Views.Windows.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:NullableFox.AoXiangToDoList.Views.Windows"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:page="using:NullableFox.AoXiangToDoList.Views.Pages"
    xmlns:uc="using:NullableFox.AoXiangToDoList.Views.UserControls"
    xmlns:winex="using:WinUIEx"
    MinWidth="350"
    MinHeight="400"
    mc:Ignorable="d">
    <Window.SystemBackdrop>
        <MicaBackdrop />
    </Window.SystemBackdrop>
    <Grid x:Name="rootGird">
        <TextBlock
            Margin="40,7,0,7"
            HorizontalAlignment="Left"
            Text="{x:Bind Title, Mode=OneWay}" />
        <page:MainPage NavigationCacheMode="Required" />
    </Grid>

</winex:WindowEx>
